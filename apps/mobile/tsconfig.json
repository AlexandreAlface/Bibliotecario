{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020"],
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@bibliotecario/ui-mobile": [
        "../../packages/ui-mobile/dist"
      ],
      "@bibliotecario/ui-mobile/*": [
        "../../packages/ui-mobile/dist/*"
      ]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
