{
  "name": "bibliotecario",
  "private": true,
  "workspaces": ["apps/*", "packages/*"],
   "scripts": {
    "dev:mobile": "npm -w apps/mobile run start",
    "dev:mobile:tunnel": "npm -w apps/mobile run start:tunnel",
    "dev:web": "npm -w @bibliotecario/apps-web run dev",
    "build:ui-web": "npm -w @bibliotecario/ui-web run build",
    "watch:ui-web": "npm -w @bibliotecario/ui-web run watch",
    "build:ui-mobile": "npm -w @bibliotecario/ui-mobile run build",
    "watch:ui-mobile": "npm -w @bibliotecario/ui-mobile run watch"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@types/react": "18.3.4",
    "@types/react-dom": "18.3.0"
  }
}
