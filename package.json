{
  "name": "bibliotecario",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:mobile": "npm -w apps/mobile run start",
    "dev:mobile:tunnel": "npm -w apps/mobile run start:tunnel",
    "dev:web": "npm -w @bibliotecario/apps-web run dev",
    "build:ui-web": "npm -w @bibliotecario/ui-web run build",
    "watch:ui-web": "npm -w @bibliotecario/ui-web run watch",
    "build:ui-mobile": "npm -w @bibliotecario/ui-mobile run build",
    "watch:ui-mobile": "npm -w @bibliotecario/ui-mobile run watch",
    "storybook": "storybook dev -p 6006 -c packages/ui-web/.storybook",
    "build-storybook": "storybook build -c packages/ui-web/.storybook -o packages/ui-web/storybook-static"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@types/react": "18.3.4",
    "@types/react-dom": "18.3.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "storybook": "^8.6.14",
    "vite": "^5.4.19"
  }
}
