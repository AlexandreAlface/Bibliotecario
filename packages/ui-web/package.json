    "react-dom": ">=18",
    "@mui/material": "^7",
    "@mui/system": "^7",
    "@mui/icons-material": "^7",
    "@emotion/react": "^11",
    "@emotion/styled": "^11"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "rimraf": "^6.0.0",
    "typescript": "~5.8.3",
    "@types/react": "18.3.4",
    "@types/react-dom": "18.3.0"
  }
